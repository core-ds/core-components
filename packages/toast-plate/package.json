    },
    "publishConfig": {
        "access": "public",
        "directory": "dist"
    },
    "peerDependencies": {
        "react": "^16.9.0 || ^17.0.1 || ^18.0.0",
        "react-dom": "^16.9.0 || ^17.0.1 || ^18.0.0"
    },
    "dependencies": {
        "@alfalab/core-components-badge": "^5.1.0",
        "@alfalab/core-components-icon-button": "^6.1.1",
        "@alfalab/core-components-mq": "^4.1.4",
        "@alfalab/icons-glyph": "^2.108.0",
        "classnames": "^2.3.1",
        "tslib": "^2.4.0"
    }
}
