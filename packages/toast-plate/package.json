{
    "name": "@alfalab/core-components-toast-plate",
    "version": "5.1.22",
    "description": "Toast plate component",
    "keywords": [],
    "license": "MIT",
    "main": "index.js",
    "module": "./esm/index.js",
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./index.js"
        },
        "./mobile": {
            "import": "./esm/mobile.js",
            "require": "./mobile.js"
        },
        "./desktop": {
            "import": "./esm/desktop.js",
            "require": "./desktop.js"
        },
        "./esm": "./esm/index.js",
        "./cssm": "./cssm/index.js",
        "./modern": "./modern/index.js",
        "./esm/*": "./esm/*",
        "./cssm/*": "./cssm/*",
        "./modern/*": "./modern/*"
