{
    "name": "@alfalab/core-components-system-message",
    "version": "1.0.2",
    "description": "System message component",
    "keywords": [],
    "license": "MIT",
    "main": "index.js",
    "module": "./esm/index.js",
    "exports": {
        ".": {
            "import": "./esm/index.js",
            "require": "./index.js"
        },
        "./mobile": {
            "import": "./esm/mobile.js",
            "require": "./mobile.js"
        },
        "./desktop": {
            "import": "./esm/desktop.js",
            "require": "./desktop.js"
        },
        "./esm": "./esm/index.js",
        "./cssm": "./cssm/index.js",
        "./modern": "./modern/index.js",
        "./esm/*": "./esm/*",
        "./cssm/*": "./cssm/*",
        "./modern/*": "./modern/*"
    },
    "scripts": {
        "postinstall": "node -e \"if (require('fs').existsSync('./send-stats.js')){require('./send-stats.js')} \""
    },
    "publishConfig": {
        "access": "public",
        "directory": "dist"
    },
    "peerDependencies": {
        "react": ">=16.8.0"
    },
    "dependencies": {
        "@alfalab/core-components-mq": "^4.1.4",
        "classnames": "^2.3.1",
        "tslib": "^2.4.0"
    }
