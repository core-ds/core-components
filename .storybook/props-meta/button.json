        "defaultValue": "false",
        "description": "Показать лоадер"
      },
      {
        "name": "nowrap",
        "type": "boolean",
        "required": false,
        "defaultValue": "false",
        "description": "Не переносить текст кнопки на новую строку"
      },
      {
        "name": "colors",
        "type": "\"default\" | \"inverted\"",
        "required": false,
        "description": "Набор цветов для компонента"
      },
      {
        "name": "allowBackdropBlur",
        "type": "boolean",
        "required": false,
